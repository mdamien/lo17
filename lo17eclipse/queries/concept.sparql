PREFIX rdfs:    <http://www.w3.org/2000/01/rdf-schema#> 
PREFIX rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> 
PREFIX owl:     <http://www.w3.org/2002/07/owl#> 

SELECT ?concept ?relation ?attribut 
WHERE {
	?concept rdf:type owl:Class . # Question a
	?relation rdf:type owl:ObjectProperty . # Question b
	?attribut rdf:type owl:DatatypeProperty . # Question c
}